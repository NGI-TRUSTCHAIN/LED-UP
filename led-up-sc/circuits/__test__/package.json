{
  "name": "zkp-tests",
  "version": "1.0.0",
  "description": "ZKP Verification Tests",
  "scripts": {
    "test:hash": "node --experimental-modules hash/run_hash_verification.js",
    "test:hash:invalid": "node --experimental-modules hash/run_invalid_proof.js",
    "test:age": "node --experimental-modules age/run_age_verification.js",
    "test:age:invalid": "node --experimental-modules age/run_invalid_age_proof.js",
    "test:age:enhanced": "node --experimental-modules age/enhanced/run_enhanced_age_verification.js",
    "test:fhir": "node --experimental-modules fhir/run_fhir_verification.js",
    "test:fhir:invalid": "node --experimental-modules fhir/run_invalid_fhir_proof.js",
    "test:fhir:enhanced": "node --experimental-modules fhir/enhanced/run_enhanced_fhir_verification.js",
    "test:fhir:enhanced:types": "node --experimental-modules fhir/enhanced/run_resource_type_tests.js",
    "test:fhir:enhanced:fields": "node --experimental-modules fhir/enhanced/run_required_fields_tests.js",
    "test:all": "npm run test:hash && npm run test:hash:invalid && npm run test:age && npm run test:age:invalid && npm run test:age:enhanced && npm run test:fhir && npm run test:fhir:invalid && npm run test:fhir:enhanced && npm run test:fhir:enhanced:types && npm run test:fhir:enhanced:fields",
    "test:ts": "mocha -t 1000000 --require ts-node/register 'circom/**/*.test.ts'",
    "test:hash:ts": "mocha -t 1000000 --require ts-node/register 'circom/HashVerifier.test.ts'",
    "test:fhir:ts": "mocha -t 1000000 --require ts-node/register 'circom/FhirVerifier.test.ts'",
    "test:all:ts": "yarn test:age:ts && yarn test:hash:ts && yarn test:fhir:ts",
    "setup-circuits": "node scripts/setup-circuits.js",
    "test:circom": "jest --config test/jest.config.js",
    "test:circom:circuits": "jest --config test/jest.config.js test/circuits",
    "test:circom:frontend": "jest --config test/jest.config.js test/frontend",
    "test:circom:integration": "jest --config test/jest.config.js test/integration",
    "test:circom:age-verifier": "jest --config test/jest.config.js test/circuits/age-verifier",
    "test:circom:hash-verifier": "jest --config test/jest.config.js test/circuits/hash-verifier",
    "test:circom:fhir-verifier": "jest --config test/jest.config.js test/circuits/fhir-verifier",
    "test:circom:coverage": "jest --config test/jest.config.js --coverage",
    "test:circom:watch": "jest --config test/jest.config.js --watch",
    "test:basic:fhir": "cd ../ && ./scripts/run-fhir-verifier-tests.sh",
    "test:detail:age": "cd ../ && ./scripts/run-all-age-detail-tests.sh",
    "test:basic:hash": "cd ../ && ./scripts/run-hash-verifier-tests.sh",
    "test:all:fhir": "../scripts/run-all-fhir-verifier-tests.sh",
    "test:all:age": "../scripts/run-all-age-verifier-tests.sh",
    "test:all:hash": "../scripts/run-all-hash-verifier-tests.sh",
    "setup:fhir": "cd ../ && ./scripts/setup-fhir-verifier.sh",
    "setup:hash": "cd ../ && ./scripts/setup-hash-verifier.sh"
  },
  "dependencies": {
    "@types/snarkjs": "^0.7.9",
    "chai": "^4.3.7",
    "circom_tester": "^0.0.20",
    "circomlibjs": "^0.1.8",
    "ethers": "^5.7.2",
    "js-sha256": "^0.9.0",
    "zokrates-js": "^1.1.9"
  },
  "devDependencies": {
    "@types/chai": "^4.3.5",
    "@types/circomlibjs": "^0.1.6",
    "@types/mocha": "^10.0.1",
    "@types/node": "^20.2.5",
    "circom": "^0.5.46",
    "circomlib": "^2.0.5",
    "mocha": "^10.2.0",
    "snarkjs": "^0.7.5",
    "ts-node": "^10.9.2",
    "typescript": "^5.0.4"
  }
}
